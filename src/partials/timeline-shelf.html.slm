a-entity.grab-root timeline-shelf="" shelf="name: Animation Shelf; closeable: true" position="-3.0 0.0 0" scale="0.3 0.3 1" visible="false"
  a-entity shelf-content=""
    a-entity icon-row="mergeButtons: true"
      a-entity icon-button="#asset-arrow-collapse-left" click-action="firstFrame" tooltip="Jump to First Frame"
      a-entity icon-button="#asset-plus-box-multiple" click-action="duplicateFrameBefore" tooltip="Duplicate Frame to Before"
      a-entity icon-button="#asset-plus-box-outline" click-action="addFrameBefore" tooltip="Insert Frame Before"
      a-entity icon-button="#asset-arrow-left" click-action="previousFrame" tooltip="Previous Frame"
      a-entity icon-button="#asset-play-pause" click-action="playPauseAnimation" tooltip="Play / Pause" speech-alias="play; pause"
      a-entity icon-button="#asset-arrow-right" click-action="nextFrame" tooltip="Next Frame"
      a-entity icon-button="#asset-plus-box-outline" click-action="addFrameAfter" tooltip="Insert Frame After"
      a-entity icon-button="#asset-plus-box-multiple" click-action="duplicateFrameAfter" tooltip="Duplicate Frame to After"
    a-entity icon-row=""
      a-entity.frame-counter icon-row-text="Frame # / ##"
      a-entity icon-row-text="@ fps: " position="1.6 0 0"
      a-entity.fps edit-field="tooltip: Frames Per Second" text="value: 10; width: 0.56; wrapCount: 4" position="2.5 0 0"

    a-entity icon-row=""
      a-entity icon-row-text="Export End Frame:"
      a-entity.number-of-frames edit-field="tooltip: End Frame Number; target: a-scene; component: timeline-system; property: endFrameNumber; autoClear: true" text="value: 10; width: 0.56; wrapCount: 4;" position="1.7 0 0"

    a-entity icon-row="mergeButtons: true"
      a-entity icon-button="#asset-camera" system-click-action="system:timeline-system; action: exportFrames" tooltip="Export Number Of Frames"
      a-entity icon-button="#asset-record" system-click-action="system:timeline-system; action: exportMP4" tooltip="Export MP4 of Number Of Frames"
      a-entity icon-button="#asset-gif" system-click-action="system:timeline-system; action: exportGif" tooltip="Export GIF of Number of Frames"
      a-entity icon-button="#asset-swap-horizontal-variant" system-click-action="system:timeline-system; action: makeUVScrollable" tooltip="**EXPERIMENTAL** Make animation UV-Scrollable"
      a-entity icon-button="#asset-badge-account-horizontal-outline" system-click-action="system: timeline-system; action: makeReference" tooltip="Make animated reference object"
      a-entity icon-button="#asset-brush" toggle-button="target: a-scene; component: compositor-history-recorder; property: recording" tooltip="Record canvas history"

    a-entity icon-row="mergeButtons: true"
      a-entity icon-button="#asset-delete" click-action="deleteFrame" tooltip="Delete Frame"
      a-entity icon-button="#asset-transition" click-action="toggleOnionSkin" tooltip="Toggle Onion Skin"
      a-entity icon-button="#asset-record" click-action="record" tooltip="Record Movie File (Start / Stop)" toggle-button="" button-style="toggleOnColor: #e99"
